
<script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        var clsGelen = {
            "Tren":
            {
                "Ad": "Başkent Ekspres",
                "Vagonlar":
                    [
                        { "Ad": "Vagon 1", "Kapasite": 10, "DoluKoltukAdet": 0 },
                        { "Ad": "Vagon 2", "Kapasite": 10, "DoluKoltukAdet": 0 },
                    ]
            },
            "RezervasyonYapilacakKisiSayisi": 14,
            "KisilerFarkliVagonlaraYerlestirilebilir": true
        };

        $.ajax({
            type: "POST",
            url: "@Url.Action("RezervasyonKontrol")",
            data: JSON.stringify(clsGelen),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (msg) {
                window.document.write(JSON.stringify(msg));
            },
            error: function (req, status, error) {
                console.log(msg);
            }
        });

    });
</script>